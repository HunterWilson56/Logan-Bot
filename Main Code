                    return message.channel.send(embed);
                } catch (err) {
                    require('hastebin-generator')(cleaned, 'js').then(link => {
                        let embed = new Discord.RichEmbed()
                            .setColor('#AABBED')
                            .setTitle('Evaluation')
                            .addField('Evaluated', '```js\n' + args.join(" ") + '```')
